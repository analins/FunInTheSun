<div id="sign-up-modal" class="modal">
    <div class="modal-content">
        <h4>Sign Up</h4>
        <form id="sign-up">
            <div class="row">
                <div class="col s6">
                    <input type="text" name="username" placeholder="username">
                </div>
                <div class="col s6">
                    <input type="password" name="password" placeholder="password">
                </div>
            </div>
            <div class="row">
                <div class="col s6">
                    <input type="text" name="name[first]" placeholder="First Name">
                </div>
                <div class="col s6">
                    <input type="text" name="name[last]" placeholder="Last Name">
                </div>
            </div>
            <div class="row">
                <input type="text" name="cities[main][name]" placeholder="city">
            </div>
            <div class="row">
                <div class="col s6">
                    <input type="text" name="cities[main][nickname]" placeholder="location nickname (optional)">
                </div>
                <div class="col s6">
                    <input type="text" name="cities[main][zipcode]" placeholder="zipcode">
                </div>
            </div>
            <div class="modal-footer">
                <input type="submit" value="Done" class="waves-effect waves-yellow btn-flat">
            </div>
        </form>
    </div>
</div>
